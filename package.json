{
  "name": "geolocation-project-backend",
  "version": "0.0.2",
  "description": "Project that explores some functionalities of a Geolocation API (Geoapify) and MongoDB's GeoJSON capabilities.",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production pnpm dlx tsx src/server.ts",
    "dev": "pnpm dlx tsx watch --include 'src/**/*' --clear-screen=false --trace-warnings --trace-uncaught src/server.ts",
    "test": "dotenvx run -- jest --testTimeout=5000",
    "build": "pnpm install && pnpm run start",
    "babel": "pnpm dlx babel src --out-dir lib",
    "lint": "ESLINT_USE_FLAT_CONFIG=true pnpm dlx eslint . --debug",
    "prettier-check": "NODE_OPTIONS='--experimental-strip-types' prettier . --check '!mongo-data'",
    "prettier-write": "NODE_OPTIONS='--experimental-strip-types' prettier . --write '!mongo-data'",
    "prepare": "husky .husky"
  },
  "author": "Naiara Correa",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@dotenvx/dotenvx": "^1.38.3",
    "@faker-js/faker": "^8.1.0",
    "@jest/globals": "^29.7.0",
    "@typegoose/typegoose": "^11.5.0",
    "@types/body-parser": "^1.19.5",
    "@types/bunyan": "^1.8.11",
    "@types/connect": "^3.4.38",
    "@types/cors": "^2.8.17",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.9.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "body-parser": "^1.20.3",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-handlebars": "^8.0.1",
    "handlebars": "^4.7.8",
    "i18next": "^24.2.2",
    "i18next-http-middleware": "^3.7.1",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "mongoose": "^7.5.3",
    "puppeteer": "^24.3.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.6",
    "vue": "^3.5.13",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-spread": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.21.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.14",
    "@types/jquery": "^3.5.32",
    "@types/node": "^20.8.0",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.1",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
