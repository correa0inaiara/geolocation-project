{
  "name": "geolocation-project",
  "version": "0.0.1",
  "description": "Project that explores some functionalities of a Geolocation API (Geoapify) and MongoDB's GeoJSON capabilities.",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node -r ts-node/register --env-file=.env src/server.ts",
    "ts-node": "ts-node src/server.ts",
    "dev": "npx tsx watch --include './src/**/*' --clear-screen=false --trace-warnings --trace-uncaught src/server.ts",
    "test": "dotenvx run -- jest --testTimeout=5000",
    "build-app": "npm run build-client && npm install && npm run build-start",
    "build-client": "cd client && npm run build-app && cd ..",
    "build-start": "pm2 start src/server.ts -n Convertible9000 && pm2 monit",
    "pm2": "pm2 start ecosystem.config.js",
    "babel": "npx babel src --out-dir lib",
    "lint": "ESLINT_USE_FLAT_CONFIG=true npx eslint . --debug",
    "prettier-check": "NODE_OPTIONS='--experimental-strip-types' prettier --check .",
    "prettier-write": "NODE_OPTIONS='--experimental-strip-types' prettier --write .",
    "prepare": "husky"
  },
  "author": "Naiara Correa",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@dotenvx/dotenvx": "^1.38.3",
    "@faker-js/faker": "^8.1.0",
    "@jest/globals": "^29.7.0",
    "@typegoose/typegoose": "^11.5.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/swagger-ui-express": "^4.1.8",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "body-parser": "^1.20.3",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-handlebars": "^8.0.1",
    "handlebars": "^4.7.8",
    "i18next": "^24.2.2",
    "i18next-http-middleware": "^3.7.1",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "mongoose": "^7.5.3",
    "puppeteer": "^24.3.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.6",
    "vue": "^3.5.13",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-spread": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.21.0",
    "@types/express": "^4.17.18",
    "@types/jquery": "^3.5.32",
    "@types/node": "^20.8.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.1",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.26.0"
  }
}
